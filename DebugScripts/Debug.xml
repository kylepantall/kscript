<kscript dynamic_defs="yes" throw_exceptions="no" quiet="no" print_info="no">
    <def id="z">20</def>

    <for to="x" math="$x->increment()" while="$x%$z->lte()">
        <for to="y" math="$y->increment()" while="@more_less($y,'lte',10)">
            <echo>$x - $y\n</echo>
        </for>
    </for>
</kscript>